{
  "name": "React Star Rating Component by Jimmy Lee",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/scss/style.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/scss/style.scss -o src/ --watch --recursive",
    "start": "concurrently \"react-scripts-ts start\" \"npm run watch-css\"",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.47",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/classnames": "^2.2.3",
    "node-sass-chokidar": "0.0.3",
    "concurrently": "^3.1.0",
    "classnames": "2.2.5",
    "shell-csslib": "2.1.0",
    "typescript": "2.3.3"
  }
}